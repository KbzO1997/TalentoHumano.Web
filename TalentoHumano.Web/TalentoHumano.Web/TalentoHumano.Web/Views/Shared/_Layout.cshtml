
<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>Talento Humano UI</title>
    <meta content='width=device-width, initial-scale=1.0, shrink-to-fit=no' name='viewport' />
    <link rel="icon" href="~/Content/img/icon.ico" type="image/x-icon" />
    <!-- Fonts and icons -->

    <script src="~/Scripts/js/plugin/webfont/webfont.min.js"></script>
    <script>
        WebFont.load({
            google: { "families": ["Lato:300,400,700,900"] },
            custom: { "families": ["Flaticon", "Font Awesome 5 Solid", "Font Awesome 5 Regular", "Font Awesome 5 Brands", "simple-line-icons"], urls: ['/Content/css/fonts.min.css'] },
            active: function () {
                sessionStorage.fonts = true;
            }
        });
    </script>

    <!-- CSS Files -->
    <link rel="stylesheet" href="~/Content/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/Content/css/atlantis.min.css">
    <!-- CSS Just for demo purpose, don't include it in your project -->
    <link rel="stylesheet" href="~/Content/css/demo.css">
    <link href="~/Content/css/appTalentoHumano.css" rel="stylesheet" />
</head>
<body>
    <div class="wrapper">
        <div class="main-header">
            <!-- Logo Header -->
            <div class="logo-header" data-background-color="blue">

                <a href="index.html" class="logo">
                    <img src="~/Content/img/logo.svg" alt="navbar brand" class="navbar-brand">
                </a>
                <button class="navbar-toggler sidenav-toggler ml-auto" type="button" data-toggle="collapse" data-target="collapse" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon">
                        <i class="icon-menu"></i>
                    </span>
                </button>
                <button class="topbar-toggler more"><i class="icon-options-vertical"></i></button>
                <div class="nav-toggle">
                    <button class="btn btn-toggle toggle-sidebar">
                        <i class="icon-menu"></i>
                    </button>
                </div>
            </div>
            <!-- End Logo Header -->
            <!-- Navbar Header -->
            <nav class="navbar navbar-header navbar-expand-lg" data-background-color="blue2">

                <div class="container-fluid">
                    <ul class="navbar-nav topbar-nav ml-md-auto align-items-center">
                        <li class="nav-item toggle-nav-search hidden-caret">
                            <a class="nav-link" data-toggle="collapse" href="#search-nav" role="button" aria-expanded="false" aria-controls="search-nav">
                                <i class="fa fa-search"></i>
                            </a>
                        </li>
                        <li class="nav-item dropdown hidden-caret">
                            <a class="nav-link cursor-pointer" role="button" onclick="SistemaInterno.VistaContactos();">
                                <i class="fa fa-envelope mr-2"></i>Contacto
                            </a>
                        </li>
                        <li class="nav-item dropdown hidden-caret">
                            <a class="nav-link dropdown-toggle" role="button" onclick="SistemaInterno.VistaNotificacion();">
                                <i class="fa fa-bell"></i>
                            </a>
                        </li>

                        <li class="nav-item dropdown hidden-caret">
                            <!--CerrarSession-->
                            <a class="nav-link cursor-pointer" role="button" onclick="SistemaInterno.VistaContactos();">
                                <i class="fa fa-user mr-2"></i>Cerrar Sesión
                            </a>
                        </li>
                    </ul>
                </div>
            </nav>
            <!-- End Navbar -->
        </div>

        <!-- Sidebar -->
        <div class="sidebar sidebar-style-2">
            <div class="sidebar-wrapper scrollbar scrollbar-inner">
                <div class="sidebar-content">
                    <div class="user">
                        <div class="avatar-sm float-left mr-2">
                            <img src="~/Content/img/profile.jpg" alt="..." class="avatar-img rounded-circle">
                        </div>
                        <div class="info">
                            <a data-toggle="collapse" href="#collapseExample">
                                <span>
                                    Carolina Jimenez
                                    <span class="user-level">Administrator</span>
                                    <span class="caret"></span>
                                </span>
                            </a>
                            <div class="clearfix"></div>
                        </div>
                    </div>
                    
                    <ul class="nav nav-primary">
                        <li class="nav-item active">
                            <a href="@Url.Action("DashBoard", "Home")" >
                                <i class="fas fa-home"></i>
                                <p>DashBoard</p>
                            </a>
                        </li>
                        <li class="nav-section">
                            <span class="sidebar-mini-icon">
                                <i class="fa fa-ellipsis-h"></i>
                            </span>
                            <h4 class="text-section">Components</h4>
                        </li>
                        <li class="nav-item">
                            <i class="fas fa-layer-group"></i> @Html.ActionLink("Administración de Solicitudes", "index", "", new { area = "" }, new { @class = "collapsed" })

                            <div class="collapse show" id="base">
                                <ul class="nav nav-collapse">
                                    <li>
                                        @Html.ActionLink("Personal", "index", "", new { area = "" }, new { @class = "collapsed" })
                                    </li>
                                    <li>
                                        @Html.ActionLink("Equipo", "index", "", new { area = "" }, new { @class = "collapsed" })
                                    </li>
                                </ul>
                            </div>
                        </li>
                        <li class="nav-item">
                            @Html.ActionLink("Administración de Tiempo", "index", "", new { area = "" }, new { @class = "collapsed" })
                        </li>
                        <li class="nav-item">
                            @Html.ActionLink("Evaluaciones", "index", "", new { area = "" }, new { @class = "collapsed" })
                        </li>
                        <li class="nav-item">
                            @Html.ActionLink("Documentos", "index", "", new { area = "" }, new { @class = "nav-link" })
                        </li>
                        <li class="nav-item">
                            @Html.ActionLink("Configuración", "index", "", new { area = "" }, new { @class = "nav-link" })
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <!-- End Sidebar -->
        <!--Modal Recursivo Ini-->
        <div class="modal fade" id="modalDinamic" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
            <div class="modal-dialog modal-lg">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="btn-close botonModalClose" data-dismiss="modal">X</button>
                    </div>
                    <div class="modal-body">
                        <div class="DivContentModal"></div>
                    </div>
                </div>
            </div>
        </div>
        <!--Modal Recursivo Fin-->

        <div class="main-panel">
            <div class="content">
                @RenderBody()
            </div>
            <footer class="footer">
                <div class="container-fluid clearfix text-center">
                    <span class="text-muted d-block d-sm-inline-block">Copyright © DesarrolloAppCentric 2021</span>
                </div>
            </footer>
        </div>

    </div>
    <!--   Core JS Files   -->
    <script src="~/Scripts/js/core/jquery.3.2.1.min.js"></script>
    <script src="~/Scripts/js/core/popper.min.js"></script>
    <script src="~/Scripts/js/core/bootstrap.min.js"></script>

    <!-- jQuery UI -->
    <script src="~/Scripts/js/plugin/jquery-ui-1.12.1.custom/jquery-ui.min.js"></script>
    <script src="~/Scripts/js/plugin/jquery-ui-touch-punch/jquery.ui.touch-punch.min.js"></script>
    <!-- jQuery Scrollbar -->
    <script src="~/Scripts/js/plugin/jquery-scrollbar/jquery.scrollbar.min.js"></script>
    <!-- Chart JS -->
    <script src="~/Scripts/js/plugin/chart.js/chart.min.js"></script>
    <!-- jQuery Sparkline -->
    <script src="~/Scripts/js/plugin/jquery.sparkline/jquery.sparkline.min.js"></script>
    <!-- Chart Circle -->
    <script src="~/Scripts/js/plugin/chart-circle/circles.min.js"></script>
    <!-- Datatables -->
    <script src="~/Scripts/js/plugin/datatables/datatables.min.js"></script>
    <!-- Bootstrap Notify -->
    <script src="~/Scripts/js/plugin/bootstrap-notify/bootstrap-notify.min.js"></script>
    <!-- Sweet Alert -->
    <script src="~/Scripts/js/plugin/sweetalert/sweetalert.min.js"></script>
    <!-- Atlantis JS -->
    <script src="~/Scripts/js/atlantis.min.js"></script>
    <!-- Atlantis DEMO methods, don't include it in your project! -->
    <script src="~/Scripts/js/setting-demo.js"></script>
    <script src="~/Scripts/js/demo.js"></script>
    <!-- Mi Scripts! -->
    <script src="~/Scripts/personalizado/Recursivo.js"></script>
    <script src="~/Scripts/personalizado/SistemaInterno.js"></script>
</body>
</html>